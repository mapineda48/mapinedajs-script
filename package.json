{
  "name": "mapineda-react",
  "version": "0.0.25",
  "private": true,
  "author": "Miguel Angel Pineda Vega",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mapineda48/mapineda-react/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/mapineda48/mapineda-react.git"
  },
  "engines": {
    "node": "^12.18.3",
    "yarn": "^1.22.5"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/fs-extra": "^9.0.5",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.26",
    "exact": "^0.8.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "4.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4",
    "webpack": "4.44.2"
  },
  "scripts": {
    "dev:bin": "ts-node -P tsconfig.node.json bin/index.ts",
    "start": "ts-node -P tsconfig.node.json bin/index.ts --start",
    "start:bin": "node dist/bin --start",
    "build:bin": "tsc -P tsconfig.node.json",
    "build:jsx": "tsc -P tsconfig.jsx.json",
    "build": "yarn build:bin && yarn build:jsx && yarn tasks",
    "test": "react-scripts test",
    "ts": "ts-node -P tsconfig.node.json",
    "tasks": "ts-node -P tsconfig.node.json tasks/index.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node -P tsconfig.node.json"
    }
  },
  "react-scripts": {
    "main": {
      "entry": "src/mock-entry/index.tsx",
      "url": "./",
      "output": "build"
    },
    "foo": {
      "entry": "src/mock-entry/foo.tsx",
      "url": "./",
      "output": "build/foo"
    },
    "default": "main"
  },
  "devDependencies": {
    "webpack-cli": "^4.5.0"
  }
}