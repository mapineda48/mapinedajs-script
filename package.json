    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "ts-node": "^9.0.0",
    "typescript": "~3.7.2"
  },
  "devDependencies": {},
  "scripts": {
    "start": "ts-node -P tsconfig.dist.json src/bin/index.ts",
    "build": "tsc -P tsconfig.dist.json",
    "react-scripts": "ts-node -P tsconfig.dist.json src/bin/react-scripts.ts",
    "publish:it": "ts-node -P tsconfig.dist.json src/bin/publish.ts",
    "pack:it": "ts-node -P tsconfig.dist.json src/bin/publish.ts --pack"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node -P tsconfig.dist.json"
    }
  }
}